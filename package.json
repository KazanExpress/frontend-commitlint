{
  "name": "@kazanexpress/frontend-commitlint",
  "version": "1.0.2",
  "description": "A commit linter for projects of KazanExpress frontend division",
  "main": "index.js",
  "bin": "index.js",
  "author": "Raiondesu",
  "license": "Proprietary",
  "scripts": {
    "preinstall": "git config core.hooksPath .githooks",
    "postinstall": "git config core.hooksPath ./node_modules/@kazanexpress/frontend-commitlint/hooks && chmod -v +x ./hooks/commit-msg"
  },
  "dependencies": {
    "@commitlint/cli": "^7.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
